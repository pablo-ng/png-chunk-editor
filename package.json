{
  "name": "png-chunk-editor",
  "version": "1.0.1",
  "description": "NPM package to read and edit chunks contained in a PNG file",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {}
  },
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pablo-ng/png-chunk-editor.git"
  },
  "keywords": [
    "PNG",
    "Chunk",
    "Chunks",
    "Metadata",
    "Tags",
    "IHDR",
    "PLTE",
    "IDAT",
    "IEND",
    "tEXt",
    "zTXt",
    "iTXt"
  ],
  "author": {
    "name": "pablo-ng",
    "email": "pablonavarrogillissen@gmail.com"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pablo-ng/png-chunk-editor/issues"
  },
  "homepage": "https://github.com/pablo-ng/png-chunk-editor#readme",
  "devDependencies": {
    "@types/jest": "29.5.3",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "eslint": "8.45.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-config-standard-with-typescript": "36.1.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-n": "16.0.1",
    "eslint-plugin-promise": "6.1.1",
    "jest": "29.6.1",
    "ts-jest": "29.1.1",
    "tsup": "7.1.0",
    "typescript": "5.1.6"
  }
}
